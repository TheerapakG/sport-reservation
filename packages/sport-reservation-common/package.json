{
  "name": "sport-reservation-common",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "import": "./dist/client/*.mjs",
      "require": "./dist/client/*.mjs"
    },
    "./db/schema": {
      "types": "./dist/db/schema.d.ts",
      "import": "./dist/db/schema.mjs",
      "require": "./dist/db/schema.mjs"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.mjs",
      "require": "./dist/models/*.mjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.mjs",
      "require": "./dist/utils/*.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "pnpm prettier -c .",
    "lint": "pnpm eslint --flag unstable_ts_config .",
    "typecheck": "pnpm tsc --noEmit",
    "build": "unbuild",
    "generate": "unbuild"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "peerDependencies": {
    "arktype": "catalog:",
    "drizzle-orm": "catalog:",
    "effect": "catalog:",
    "hookable": "^5.5.3",
    "nitropack": "catalog:",
    "ofetch": "catalog:",
    "pathe": "catalog:",
    "ufo": "catalog:",
    "unbuild": "catalog:"
  },
  "devDependencies": {
    "@eslint/compat": "catalog:",
    "@eslint/eslintrc": "catalog:",
    "@eslint/js": "catalog:",
    "@types/eslint__eslintrc": "catalog:",
    "@types/eslint__js": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "drizzle-kit": "^0.24.2",
    "eslint": "catalog:",
    "glob": "^11.0.0",
    "globals": "catalog:",
    "prettier": "catalog:"
  }
}
